{
  "name": "jsfitsio",
  "description": "FITS I/O javascript library.",
  "version": "1.2.0",
  "keywords": [
    "FITS",
    "FITS I/O",
    "FITS in a browser"
  ],
  "author": "Fabrizio Giordano (fab77)",
  "license": "MIT",
  "homepage": "https://github.com/fab77/FITSParser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fab77/FITSParser.git"
  },
  "bugs": {
    "url": "https://github.com/fab77/FITSParser/issues",
    "email": "fabriziogiordano77@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "import": "./lib-esm/index.js",
      "require": "./dist/jsfitsio.js"
    }
  },
  "main": "./lib-esm/index.js",
  "types": "./lib-esm/index.d.ts",
  "files": [
    "lib-esm/",
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist lib-esm",
    "start:dev": "npm run clean && tsc && webpack --mode=development --watch --progress; cp dist/* webtest/",
    "build:prod": "npm run clean && tsc && webpack --mode=production",
    "webtest": "cp dist/* webtest/; node server.cjs",
    "test:esm": "tsc -m es6 test.ts; node test.js",
    "test": "jest",
    "test2": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.76",
    "jest": "^29.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "fs": "^0.0.1-security",
    "ts-node": "^10.9.2"
  },
  "browser": {
    "fs": false,
    "node:fs/promises": false,
    "node:fs": false,
    "fs/promises": false
  }
}
