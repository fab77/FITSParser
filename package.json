{
  "name": "jsfitsio",
  "description": "FITS I/O javascript library.",
  "version": "1.2.2",
  "keywords": [
    "FITS",
    "FITS I/O",
    "FITS in a browser",
    "astronomy",
    "astrophysics",
    "science",
    "data format"
  ],
  "author": "Fabrizio Giordano (fab77)",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/fab77/jsfitsio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fab77/jsfitsio.git"
  },
  "bugs": {
    "url": "https://github.com/fab77/jsfitsio/issues",
    "email": "fabriziogiordano77@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib-esm/index.d.ts",
      "import": "./lib-esm/index.js",
      "require": "./dist/jsfitsio.cjs"
    }
  },
  "module": "./lib-esm/index.js",
  "main": "./dist/jsfitsio.cjs",
  "types": "./lib-esm/index.d.ts",

  "unpkg": "dist/jsfitsio.min.js",
  "jsdelivr": "dist/jsfitsio.min.js",
  "files": [
    "lib-esm/",
    "dist"
  ],
  "prepublishOnly": "npm run build:prod",
  "sideEffects": false,
  "scripts": {
    "clean": "shx rm -rf dist lib-esm",
    "dev": "npm run clean && tsc && webpack --mode=development --watch --progress; cp dist/* webtest/",
    "prod": "npm run clean && tsc && webpack --mode=production",
    "webtest": "cp dist/* webtest/; node server.cjs",
    "test:esm": "tsc -m es6 test.ts; node test.js",
    "test": "jest",
    "test2": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.76",
    "jest": "^29.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "browser": {
    "fs": false,
    "node:fs/promises": false,
    "node:fs": false,
    "fs/promises": false
  }
}